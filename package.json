{
  "name": "@italojs/herbs-cli-alpha",
  "version": "0.0.2",
  "description": "herbs CLI",
  "bin": {
    "herbs": "bin/herbs"
  },
  "scripts": {
    "format": "prettier --write **/*.{js,json} && standard --fix",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "test": "mocha __tests__ --timeout 90000",
    "watch": "jest --watch",
    "snapupdate": "jest --updateSnapshot",
    "coverage": "jest --coverage"
  },
  "files": [
    "src",
    "LICENSE",
    "readme.md",
    "docs",
    "bin"
  ],
  "license": "MIT",
  "dependencies": {
    "gluegun": "latest",
    "gotu": "^1.0.15",
    "inquirer": "^8.1.1",
    "lodash.camelcase": "^4.3.0",
    "lodash.startcase": "^4.4.0"
  },
  "devDependencies": {
    "prettier": "^1.12.1",
    "standard": "^12.0.1",
    "mocha": "^9.0.0",
    "chai": "^4.3.4"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "standard": {
    "env": [
      "it",
      "describe"
    ]
  }
}
