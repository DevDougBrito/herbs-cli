const { entity2type } = require('herbs2gql')
const entitiesObj = require('../../../domain/entities')

const defaultSchema = [`
  type Query {
    _: Boolean
  }

  type Mutation {
    _: Boolean
  }`]

let types = [defaultSchema]
const entities = Object.keys(entitiesObj).map(entity => [entity2type(entities[entity])])
types = types.concat(entities)

module.exports = types