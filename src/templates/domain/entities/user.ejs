const { entity, id, field } = require('@herbsjs/herbs')
const { herbarium } = require('@herbsjs/herbarium')

const User =
        entity('User', {
          <% if(props.mongo) {%>id: id(String),<% }%><% if(!props.mongo) {%>id: id(Number),<% }%>
          nickname: field(String),
          password: field(String)
        })

module.exports =
  herbarium.entities
    .add(User, 'User')
    .entity
